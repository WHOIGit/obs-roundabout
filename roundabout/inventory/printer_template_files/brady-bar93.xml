<?xml version = "1.0" ?>
<bpl-document xmlns = "http://www.bradycorp.com/printers/bpl">
    <defaults>
        <document units = "inches" />
        <printer match-media="B33-27-427" match-ribbon="B30-R4300" />
    </defaults>
    <labels>
        <label width = "2.0" height = "1.0" font-size="10" font-name="Arial" orientation="landscape">
            <barcode
            position-x = "0.0"
            position-y = "0.2"
            height = "0.3"
            type = "code 93"
            human-readable = "false"
            human-readable-location = "bottom"
            density="10"
            ratio="1:1"
            check-character= "false" >
                <datasource>
                    <static-text value = "" />
                </datasource>
            </barcode>
            <text position-x="0.1" position-y="0.6" align="center">
                <text-sizing>
                    <freeform height="0.25" />
                </text-sizing>
                <datasource>
                    <static-text value="" />
                </datasource>
            </text>

            <text position-x="0.1" position-y="0.8" align="center">
                <text-sizing>
                    <!--<manual height="0.2" width="1.2" font-size="8" />
                    <autosize height="0.2" width="1.0" />-->

                    <freeform height="0.25" />
                </text-sizing>
                <datasource>
                    <static-text value="" />
                </datasource>
            </text>

        </label>
    </labels>
</bpl-document>
