<?xml version = "1.0" ?>
<bpl-document xmlns = "http://www.bradycorp.com/printers/bpl">
    <defaults>
        <document units = "inches" />
    </defaults>
    <labels>
        <label width = "2.0" height = "1.0" font-size="8" font-name="Arial"> \
            <barcode
            position-x = "0.1"
            position-y = "0.2"
            height = "0.6"
            type = "qr-code"
            human-readable = "true"
            human-readable-location = "bottom" >
                <datasource>
                    <static-text value = "" />
                </datasource>
            </barcode>
            <text position-x="0.2" position-y="0.5" align="center">
                <text-sizing>
                    <autosize height="0.2" width="1.5" />
                </text-sizing>
                <datasource>
                    <static-text value="" />
                </datasource>
            </text>

            <text position-x="0.72" position-y="0.4" align="right">
                <text-sizing>
                    <!--<manual height="0.2" width="1.2" font-size="8" />
                    <autosize height="0.2" width="1.0" />-->

                    <freeform height="0.3" />
                </text-sizing>
                <datasource>
                    <static-text value="" />
                </datasource>
            </text>
        </label>
    </labels>
</bpl-document>
